<div id="sign-in-page">
	<img src="<%= image_url("starting.png")%>" id="image">
	<div id="sign-up-box">
		<img src="<%= image_url("quora.jpg")%>" id="sign-in-page-quora-image" >
		<div>A place to share knowledge and better understand the world</div>
		<div id="sign-in-box-inner">
			<!-- <div id="sign-in-with-apis">
				<div id="sign-in-with-google">Continue With Google</div>
				<div id="sign-in-with-google">Continue With Facebook</div>
			</div> -->
			<%= form_tag({controller: "authentication_system",action: "sign_up"},method: :post) do %>
				<%= text_field_tag(:name,nil,placeholder:"name") %><br>
				<%= email_field_tag(:email,nil,placeholder:"Email") %><br>
				<%= password_field_tag(:password,nil,placeholder:"Password") %><br>
				<%= submit_tag("Sign Up") %>
			<% end %>
		</div>
	</div>

</div>	


	